{
    "launcher": {
        "args": {
            "sensorList": [
                {
                    "sensorType": "intMaker",
                    "sensorID": 1,
                    "globalID": 2
                }
            ]
        }
    },
    "commandControl": {
        "args": {
            "heartBeatInterval": 60,
            "commandServerEndPoint": "http://10.84.141.230/command/device/getCommands"
        }
    },
    "dataControl": {
        "args": {
            "updateInterval": 2,
            "dataIngestion_URL": "http://10.84.141.230/data/device/dataIngestion",
            "destination": "self.localData/dataRepo.json",
            "localData": "Local_Data/",
            "localFileName": "localCSV.csv",
            "headers": [
                "data",
                "timeStamp",
                "dataType",
                "units",
                "sensorType",
                "sensorID"
            ]
        }
    },
    "managementController": {
        "args": {
            "updateInterval": 1,
            "registrationURL": "http://10.84.141.230/management/device/supervisorRegistration"
        }
    },
    "logLevel": "DEBUG",
    "test": false,
    "localOnly": false,
    "threadLimit": 10,
    "deviceID": 1
}