{
    "launcher": {
        "args": {
            "sensorList": [
                {
                    "sensorType": "intMaker",
                    "sensorID": 2
                }
            ]
        }
    },
    "commandControl": {
        "args": {
            "heartBeatInterval": 60,
            "commandServerEndPoint": "http://0.0.0.0/command/device/getCommands"
        }
    },
    "dataControl": {
        "args": {
            "updateInterval": 2,
            "dataIngestion_URL": "http://0.0.0.0/data/device/dataIngestion",
            "destination": "self.localData/dataRepo.json",
            "localData": "Local_Data/",
            "localFileName": "localCSV.csv",
            "headers": ["data", "timeStamp", "dataType", "units", "sensorType", "sensorID"]

        }
    },
    "managementController": {
        "args": {
            "updateInterval": 1,
            "registrationURL": "http://0.0.0.0/management/device/supervisorRegistration"
        }
    },
    "logLevel": "DEBUG",
    "test": false,
    "localOnly": false,
    "threadLimit": 10,
    "deviceID": 1
}