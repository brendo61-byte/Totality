{
  "launcher": {
    "args": {
      "sensorList": [
        {
          "sensorType": "intMaker",
          "sensorID": 1
        },
        {
          "sensorType": "ADC1115_Pyra",
          "sensorID": 2
        },
        {
            "sensorType": "BME280",
            "sensorID": 3
        }
      ]
    }
  },
  "commandControl": {
    "args": {
      "heartBeatInterval": 60,
      "commandServerEndPoint": "http://10.84.141.230/command/device/getCommands"
    }
  },
  "dataControl": {
    "args": {
      "updateInterval": 2,
      "dataIngestion_URL": "http://127.0.0.1:8000/data",
      "destination": "self.localData/dataRepo.json",
      "localData": "Local_Data/",
      "localFileName": "localCSV.csv",
      "headers": [
        "data",
        "timeStamp",
        "readingType",
        "units",
        "sensorType",
        "sensorID"
      ]
    }
  },
  "managementController": {
    "args": {
      "updateInterval": 1,
      "registrationURL": "http://10.84.141.230/management/device/supervisorRegistration"
    }
  },
  "logLevel": "DEBUG",
  "test": false,
  "localOnly": true,
  "threadLimit": 10,
  "deviceID": 1
}